        "dotenv": "^16.3.1",
        "sqs-consumer": "^7.2.2",
        "aws-sdk": "^3.0",
        "node-fetch": "^3.3.2",
        "crypto": "^1.0.1",
        "helmet": "^7.1.0",
        "compression": "^1.7.4",
