{
    "name": "v1chunker",
    "version": "1.0.0",
    "description": "VibesFlow Chunker Worker - NEAR Shade Agent for VRF Raffling",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "node --inspect server.js",
        "docker:build": "docker build -t ghcr.io/vibesflow/chunker-worker .",
        "docker:push": "docker push ghcr.io/vibesflow/chunker-worker",
        "docker:run": "docker run -p 3001:3001 ghcr.io/vibesflow/chunker-worker"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "multer": "^1.4.5-lts.1",
        "near-api-js": "^4.0.3",
        "near-seed-phrase": "^0.2.0",
        "node-fetch": "^3.3.2",
        "dotenv": "^16.3.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "near",
        "shade-agent",
        "vrf",
        "chunker",
        "vibesflow",
        "audio",
        "raffle"
    ],
    "author": "VibesFlow Team",
    "license": "MIT"
}
