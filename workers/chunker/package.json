    "description": "VibesFlow Chunker Worker - NEAR Shade Agent with VRF raffles and TEE attestation",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "build": "echo 'Building Shade Agent...' && node -e 'console.log(\"âœ… Build completed\")'",
        "test": "node --experimental-vm-modules node_modules/.bin/jest",
